{
  "name": "cost",
  "$schema": "../../node_modules/nx/schemas/project-schema.json",
  "sourceRoot": "modules/cost/src",
  "projectType": "library",
  "targets": {
    "build": {
      "executor": "@nx/vite:build",
      "outputs": ["{options.outputPath}"],
      "options": {
        "outputPath": "dist/modules/cost",
        "main": "modules/cost/src/index.ts",
        "tsConfig": "modules/cost/tsconfig.lib.json",
        "assets": ["modules/cost/*.md"]
      }
    },
    "publish": {
      "command": "node tools/scripts/publish.mjs cost {args.ver} {args.tag}",
      "dependsOn": ["build"]
    },
    "install": {
      "command": "cd modules/cost && npm install"
    }
  },
  "tags": []
}
